syntax: glob

# The following globs exclude executables built in the "tools" directory.

tools/build-pkg
tools/extract-pkg
tools/makefont
tools/pngtotex
tools/streamux
tools/*.exe
tools/tooltest*

# The following globs exclude files generated by the build process for the
# sample program in the "sample" directory.

# Common build directories
sample/build/obj
sample/build/obj-*
# Android-specific
sample/build/AndroidManifest.xml
sample/build/assets
sample/build/class
sample/build/classes.dex
sample/build/java
sample/build/libs
sample/build/res
sample/build/sample._pk
sample/build/sample.a_k
sample/build/sample.ap_
sample/build/sample.apk
# iOS/OSX-specific
sample/build/sample.app
sample/build/sample.app.dSYM
# iOS-specific
sample/build/Assets.plist
sample/build/Assets.xcassets
sample/build/bin
sample/build/sample.ipa
sample/build/sample.xcent
# Linux-specific
sample/build/sample
# PSP-specific
sample/build/sample.elf
sample/build/sample.prx
sample/build/EBOOT.PBP
sample/build/ICON0.PNG
sample/build/ICON0-alpha.pnm
sample/build/PARAM.SFO
# Windows-specific
sample/build/sample.exe
sample/build/sample.exe.ico
sample/build/sample.exe.manifest
sample/build/sample.rc
sample/build/sample.res
# Coverage testing data and results
sample/build/.covtmp
sample/build/coverage*

# These are generated by Xcode when using "sample.xcodeproj" to run the
# sample program on iOS.

sample/build/DerivedData
sample/build/sample.xcodeproj/project.xcworkspace/xcuserdata
sample/build/sample.xcodeproj/xcuserdata
